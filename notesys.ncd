ffmpeg -re -i &quot;E:\片源\复仇者联盟720p.mov&quot; -vcodec copy -acodec copy -f flv rtmp://192.168.11.75/live/test1 <BR>
ffmpeg -re -i &quot;E:\片源\复仇者联盟720p.mov&quot; -vcodec copy -acodec copy -f flv rtmpt://192.168.11.75:8080/live/test1<BR>
