	addr.sin_port = htons(UDP_TEST_PORT);<BR>
	addr.sin_addr.s_addr = htonl(INADDR_ANY) ;// 接收任意IP发来的数据<BR>
<BR>
	/* 绑定socket */<BR>
	if (bind(sockfd, (struct sockaddr *)&amp;addr, sizeof(addr))&lt;0) {<BR>
		perror(&quot;connect&quot;);<BR>
		exit(1);<BR>
	}<BR>
<BR>
	while(1) {<BR>
		bzero(buffer, sizeof(buffer));<BR>
