destroy_t* destroy_triangle = (destroy_t*) dlsym(triangle, &quot;destroy&quot;);<BR>
dlsym_error = dlerror();<BR>
if (dlsym_error) {<BR>
cerr &lt;&lt; &quot;Cannot load symbol destroy: &quot; &lt;&lt; dlsym_error &lt;&lt; '\n';<BR>
return 1;<BR>
}<BR>
