#include &lt;string.h&gt;<BR>
#include &lt;stdio.h&gt;<BR>
<BR>
#define	UDP_TEST_PORT		50001<BR>
<BR>
int main(int argC, char* arg[])<BR>
{<BR>
	struct sockaddr_in addr;<BR>
	int sockfd, len = 0;	<BR>
	int addr_len = sizeof(struct sockaddr_in);<BR>
	char buffer[256];	<BR>
<BR>
	/* 建立socket，注意必须是SOCK_DGRAM */<BR>
	if ((sockfd = socket(AF_INET, SOCK_DGRAM, 0)) &lt; 0) {<BR>
		perror (&quot;socket&quot;);<BR>
		exit(1);<BR>
	}<BR>
<BR>
