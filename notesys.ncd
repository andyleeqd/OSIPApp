#include &lt;opencv2\core\core.hpp&gt;<BR>
#include &lt;iostream&gt;<BR>
#include &lt;time.h&gt;<BR>
<BR>
using namespace cv;<BR>
using namespace std;<BR>
int main()<BR>
{<BR>
    Mat img = imread(&quot;1.jpg&quot;);<BR>
	cout&lt;&lt;&quot;img.rows: &quot;&lt;&lt;img.rows&lt;&lt;&quot; img.cols: &quot;&lt;&lt;img.cols&lt;&lt;&quot; img.channels(): &quot;&lt;&lt;img.channels()&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;###############################################&quot;&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.step1(0): &quot;&lt;&lt;img.step1(0)&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.step1(1): &quot;&lt;&lt;img.step1(1)&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.step[0]: &quot;&lt;&lt;img.step[0]&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.step[1]: &quot;&lt;&lt;img.step[1]&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.size[0]: &quot;&lt;&lt;img.size[0]&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.size[1]: &quot;&lt;&lt;img.size[1]&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.elemSize(): &quot;&lt;&lt;img.elemSize()&lt;&lt;endl;<BR>
	cout&lt;&lt;&quot;img.elemSize1(): &quot;&lt;&lt;img.elemSize1()&lt;&lt;endl;<BR>
    return 0;<BR>
}<BR>
